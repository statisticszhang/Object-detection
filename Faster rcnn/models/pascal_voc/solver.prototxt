train_net: "~/caffe-model/det/faster_rcnn/models/pascal_voc/resnet101-v2/faster_voc_resnet101-v2-merge.prototxt"
base_lr: 0.001
lr_policy: "multistep"
gamma: 0.1
stepvalue: 50000
display: 20
average_loss: 100

momentum: 0.9
weight_decay: 0.0001
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "faster_voc_resnet101-v2"
iter_size: 1
